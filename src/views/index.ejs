<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css"
        integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
        <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
    <title>AFFLUENT</title>
</head>

<body>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <div class="navbar-brand">Affluent</div>
    </nav>   
    <div class="container">
        <div class="row mt-5">
            <div class="col-md-4">
                <div class="m4">
                    <h2>Users</h2>
                </div>
                <div><button class="btn btn-info" id="btn_fill_users" type="button">fill table</button></div>
                <table class="table table-bordered table-hover">
                    <thead>
                        <tr>
                            <th>ID</th>
                            <th>Email</th>
                            <th>First Name</th>
                            <th>Last Name</th>
                            <th>Avatar</th>                            
                        </tr>
                    </thead>
                    <tbody>
                        <% if (data) { %>
                            <% for(var i = 0; i < data.length; i++) { %>
                                <tr>
                                    <td><%= data[i].ID %></td>
                                    <td><%= data[i].email %></td>
                                    <td><%= data[i].first_name %></td>
                                    <td><%= data[i].last_name %></td>
                                    <td><img src="<%= data[i].avatar %>"></td>                                    
                                </tr>
                                <% } %>
                                <% } %>
                            </tbody>
                        </table>
    <div><h2>Dates</h2></div>
       <div><button class="btn btn-info" id="btn_fill_dates" type="button">affluent table</button></div>
                <table class="table table-bordered table-hover">
                    <thead>
                        <tr>
                            <th>Date</th>
                            <th>Commissions-total</th>
                            <th>Sales-net</th>
                            <th>Leads-net</th>
                            <th>Clicks</th>
                            <th>EPC</th>
                            <th>Impressions</th>
                            <th>CR</th>                            
                        </tr>
                    </thead>
                   <!--  <tbody>
                        <% if (aff_data) { %>
                            <% for(var i = 0; i < aff_data.length; i++) { %>
                              <tr>
                                <td><%= aff_data[i].ID %></td>
                                <td><%= aff_data[i].email %></td>
                                <td><%= aff_data[i].first_name %></td>
                                <td><%= aff_data[i].last_name %></td>
                                <td><img src="<%= data[i].avatar %>"></td>                                
                              </tr>
                            <% } %>
                          <% } %>
                    </tbody> -->
                </table>
            </div>                       
        </div>
    </div>
    <script src="/main.js"></script>
</body>

</html>